import java.util.*;
import java.io.*;

public class main{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n = scn.nextInt();
        for(int i = 1; n >0; i++){
            int r = n%10;
            int q = n/10;
            n = q;
            System.out.print(r);
        }


     }
 }
